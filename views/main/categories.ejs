<h2>My Categories</h2>
<hr>

<% if (categories.length < 1) { %>
    <h4>Better get coding, I've no categories!</h4>
<% } %>

<% categories.forEach(category => { %>
    <div class="well">
        <h2>
            <a href="/categories/<%= category.id %>"><%= category.category %></a>
        </h2>
    </div>
<% }); %>
